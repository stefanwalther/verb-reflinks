{
  "name": "verb-reflinks",
  "description": "Verb middleware that resolves reflinks for valid npm package names in markdown documents and appends them to the document.",
  "version": "0.1.8",
  "homepage": "https://github.com/verbose/verb-reflinks",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "verbose/verb-reflinks",
  "bugs": {
    "url": "https://github.com/verbose/verb-reflinks/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "arr-diff": "4.0.0",
    "arr-union": "^3.1.0",
    "array-unique": "^0.3.2",
    "debug": "3.1.0",
    "expand-reflinks": "^0.2.1",
    "reflinks": "^1.0.0",
    "validate-npm-package-name": "^3.0.0"
  },
  "devDependencies": {
    "gulp-format-md": "1.0.0",
    "mocha": "5.2.0",
    "templates": "1.2.9"
  },
  "keywords": [
    "api docs",
    "code comments",
    "docs",
    "documentation",
    "generate",
    "generator",
    "jsdoc",
    "markdown",
    "plugin",
    "readme",
    "reflinks",
    "template",
    "templates",
    "verb",
    "verb-generator",
    "verbgenerator",
    "verbplugin"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "gulp-reflinks",
        "reflinks",
        "verb"
      ]
    },
    "reflinks": [
      "generate",
      "verb",
      "verb-readme-generator"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
